{
  "name": "kronos-http-routing-step",
  "version": "0.0.0-semantic-release",
  "description": "step to route http requests to endpoints",
  "keywords": [
    "kronos-step"
  ],
  "main": "lib/http_routing.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "ava",
    "semantic-release": "semantic-release",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-http-routing-step.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-http-routing-step/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-http-routing-step#readme",
  "dependencies": {
    "kronos-service-koa": "^3.3.15",
    "kronos-step": "^5.6.4"
  },
  "devDependencies": {
    "kronos-service-manager": "^3.5.10",
    "kronos-service-registry": "^1.4.10",
    "kronos-test-step": "3.1.4",
    "semantic-release": "^11.1.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "documentation": "^5.3.5",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.1",
    "ava": "^0.24.0",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-multi-entry": "^2.0.2",
    "xo": "^0.19.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.3"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  }
}
